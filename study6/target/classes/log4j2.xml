<?xml version="1.0" encoding="UTF-8"?>
<configuration>
 
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{DEFAULT} %5p [%c] %m%n"/>
        </Console>
    </Appenders>
    
    <loggers>
        <root level="DEBUG" additivity="true">
            <AppenderRef ref="console"/>
        </root>
    </loggers>
 
</configuration>
